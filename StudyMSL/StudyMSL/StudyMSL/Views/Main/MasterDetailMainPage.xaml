                  x:Class="StudyMSL.Views.Main.MasterDetailMainPage"
                  xmlns:local="clr-namespace:StudyMSL.Views.Main;assembly=StudyMSL" >



    <MasterDetailPage.Master>
        <local:MasterPage x:Name="masterPage" />
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:HomePage/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>